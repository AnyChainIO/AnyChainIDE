QMainWindow{
    background-color:rgb(31,31,31);
}

QStatusBar{
    background-color:transparent;
    color:white;
}
QStatusBar:item{
    border:none;
}

QSplitter::handle {
    background-color: rgb(31,31,31);
}


QToolBar {
    padding:0px 0px 0px 6px;
    background: #323232;border-bottom:
    1px solid rgb(31,31,31);
}
QToolBar::separator{
    width:1px;
    margin:0px 0px 0px 0px;
    background-color:
    rgb(20,20,20);
}
QMenuBar{
    padding:0px 0px 0px 10px;
    background: #323232;
    border-bottom:1px solid rgb(31,31,31);
    font: 14px \"微软雅黑\";
    color:white;
}
QMenuBar::item {
    margin:0px 0px 0px 0px;
    background-color:#323232;
    padding:7px 10px 8px 10px;
    color:white;
}
QMenuBar::item:selected {
    background:#404040;
    color:white;
}


QMenu{
    background-color:#323232;
    border:1px solid rgb(10,10,10);
}
QMenu::item{
    margin-left:4px;
    padding:8px 23px 8px 23px;
    border:1px solid transparent;
    font: 14px \"微软雅黑\";
    color:white;
}
QMenu::item:selected{
    background:#404040;
    color:white;
}
QMenu::item:disabled{
    color:gray;
}

QTreeView{
    background-color:rgb(46,46,46);
    border:none;
    color:white;
    font:65 14px \"Microsoft YaHei UI\";
    outline:0px;
}
QTreeView::branch{
    background:rgb(46,46,46);
}
QTreeView::item{
    height:32px;
}
QTreeView::item::selected{
    background:rgb(38,38,38);
    border:none;
    color:#44D9C7;
}
QTreeView::branch:selected:has-children:adjoins-item {
    border-left:0px solid rgb(0,210,255);
}
QTreeView::branch::selected::adjoins-item{
    background:rgb(38,38,38);
}
QTreeView::branch::selected::!adjoins-item{
    border-left:0px solid rgb(0,210,255);
    background:rgb(38,38,38);
}
QTreeView::branch::hover::adjoins-item,QTreeView::branch::hover::!adjoins-item,QTreeView::item::hover{
    background:rgb(38,38,38);
}
QTreeView::branch:has-children:!has-siblings:closed,QTreeView::branch:closed:has-children:has-siblings {
    border-image: none;
    image: url(:/pic/close.png);
}
QTreeView::branch:open:has-children:!has-siblings,QTreeView::branch:open:has-children:has-siblings  {
    border-image: none;
    image: url(:/pic/open.png);
}
QTreeView::item:has-children{
    color:white;
}


QTextEdit{
    background-color: rgb(46,46,46);
    color: #F1F6FE;
    font: 14px \"Microsoft YaHei UI Light\";
    border:none;
    padding: 0px 10px 0px 6px;
}

QTabWidget::pane{
    border: 0px solid #C2C7CB;
    border-top: 0px;
}


QTabBar::tab{
    background-color:rgb(46,46,46);
    color:rgb(98, 98, 98) ;
    border-bottom: 0px solid rgb(233,233,240);
    min-width:75px;
    min-height:25px;
    max-width:200px;
    font: 14px \"微软雅黑\";
    padding: 2px 5px 2px 5px;
}
QTabBar::tab:selected{
    background-color:rgb(38,38,38);
    color: rgb(250, 250, 250);
    border-bottom: 0px solid rgb(68,217,199);
}
QTabBar::close-button {
    image: url(:/pic/close2.png);
    subcontrol-origin: padding;
    subcontrol-position: bottom right;
}


QScrollBar:vertical {
     border: 0px solid rgb(71,71,71);
     background: rgb(46,46,46);
     width: 10px;
     margin: 1px 0 1px 0;
}
QScrollBar:horizontal {
     border: 0px solid rgb(71,71,71);
     background: rgb(46,46,46);
     height: 10px;
     margin: 0px 1px 0px 1px;
}
QScrollBar::handle:vertical {
   border: 0px solid rgb(71,71,71);
   border-radius:5px;
   background: rgb(25,25,25);
   min-height:30px;
}
QScrollBar::handle:horizontal {
   border: 0px solid rgb(71,71,71);
   border-radius:5px;
   background: rgb(25,25,25);
   min-width:30px;
}

QScrollBar::add-line:vertical {
    border: 0px solid rgb(71,71,71);
    background: rgb(46,46,46);
    height: 15px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}
QScrollBar::add-line:horizontal {
border: 0px solid rgb(71,71,71);
    background: rgb(46,46,46);
    width: 15px;
    subcontrol-position: right;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical {
    border: 0px solid rgb(71,71,71);
    background: rgb(46,46,46);
    height: 15px;
    subcontrol-position: top;
    subcontrol-origin: margin;
}
QScrollBar::sub-line:horizontal {
    border: 0px solid rgb(71,71,71);
    background: rgb(46,46,46);
    width: 15px;
    subcontrol-position: left;
    subcontrol-origin: margin;
}
QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical,QScrollBar::up-arrow:horizontal, QScrollBar::down-arrow:horizontal {
    border: 0px solid rgb(71,71,71);
    width: 5px;
    height: 5px;
    background: white;
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical,QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
     background: rgb(46,46,46);
}

QLineEdit{
    color:white;
    font: 14px \"Microsoft YaHei UI Light\";
    background-color:rgb(46,46,46);
    border:1px solid rgb(30,30,30);
}
QLineEdit:focus{
    border:1px solid rgb(30,30,30);
}

QLabel{
    color:white;
}
QLabel:disable{
    color:gray;
}
QComboBox{
    background-color:transparent;
    border:none;
    border-bottom:1px solid gray;
    color: white;
    padding-left: 5px;
    font: 14px \"Microsoft YaHei UI Light\";
}
QComboBox:on{
    border:0px solid rgb(93,107,144);
    color:white;
    background-color:rgb(50,50,50);
}
QComboBox::drop-down {
    background:transparent;
    border:none;
}
QComboBox::down-arrow {
    image: url(:/pic/downArrow.png);
}
QComboBox::down-arrow:on {
    image: url(:/pic/downArrow_on.png);
}
QComboBox QAbstractItemView {
    background:rgb(46,46,46);
    border:1px solid darkgray;
    color:white;
    outline:0px;
    font: 14px \"Microsoft YaHei UI Light\";
}
QComboBox QAbstractItemView::item:hover
{
    color:#44D9C7;
    background-color:rgb(38,38,38);
    border:none;
}

QComboBox QAbstractItemView::item:selected
{
    color:#44D9C7;
    background-color:rgb(38,38,38);
}

QDialog{
    background-color:rgb(50,50,50);
}

QHeaderView{
    border:1px solid rgb(70,70,70);
    background-color:rgb(50,50,50);
    color:white;
    font: 14px \"Microsoft YaHei UI Light\";
    min-height:30px;
}
QHeaderView:section{
    border:none;
    border-right: 1px solid rgb(70,70,70);
    background-color:rgb(50,50,50);
}

QDoubleSpinBox::up-button {
    width:0;
    height:0;
}
QDoubleSpinBox::down-button {
    width:0;
    height:0;
}
QDoubleSpinBox::up-arrow {
    width:0;
    height:0;
}
QDoubleSpinBox::down-arrow {
    width:0;
    height:0;
}
QDoubleSpinBox{
    background-color: transparent;
    border-top:none;
    border-left:none;
    border-right:none;
    border-bottom:1px solid gray;
    color:white;
}
QDoubleSpinBox:focus{
    border-bottom:1px solid rgb(84,116,235);
}
QDoubleSpinBox:disabled{
    background:transparent;color: rgb(83,90,109);
}


QSpinBox::up-button {
    width:0;
    height:0;
}
QSpinBox::down-button {
    width:0;
    height:0;
}
QSpinBox::up-arrow {
    width:0;
    height:0;
}
QSpinBox::down-arrow {
    width:0;
    height:0;
}
QSpinBox{
    background-color: transparent;
    border-top:none;
    border-left:none;
    border-right:none;
    border-bottom:1px solid gray;
    color:white;
}
QSpinBox:focus{
    border-bottom:1px solid rgb(84,116,235);
}
QSpinBox:disabled{
    background:transparent;color: rgb(83,90,109);
}

QCheckBox{
    color:white;
}
QCheckBox::indicator{
    image:url(:/pic/checkBox2_unchecked.png);
}
QCheckBox::indicator:checked{
    image:url(:/pic/checkBox2_checked.png);
}

QTableWidget{
    background-color:rgb(46,46,46);
    border:none;
    color:white;
    font:65 14px \"Microsoft YaHei UI\";
    outline:0px;
}
QTableWidget::item{
    height:32px;
}
QTableWidget::item::selected{
    background:rgb(38,38,38);
    border:none;
    color:#44D9C7;
}



/*具体按钮的定制*/
QLabel#title_Label{
    color:white;
    font:65 18px \"Microsoft YaHei UI\";
    background-color:rgb(70,70,70);
    padding-left:10px;
}

QToolButton#closeBtn{
    background-image:url(:/pic/close2.png);
    background-repeat: no-repeat;
    background-position: center;
    background-color:transparent;
    border:none;
}

QToolButton#okBtn,QToolButton#cancelBtn,QToolButton#clearBtn,
QToolButton#newAccount,QToolButton#importKey,
QToolButton#toolButton_checkUpdate,QToolButton#toolButton_restart,QToolButton#toolButton_update,
QToolButton#pathBtn,QToolButton#resetDataBtn
{
    border:1px solid gray;
    border-radius:10px;
    color:white;
    background-color:rgb(66,66,66);
    height:20px;
    width:60px;
}
QToolButton#okBtn:disabled,QToolButton#cancelBtn:disabled,QToolButton#clearBtn:disabled,
QToolButton#newAccount:disabled,QToolButton#importKey:disabled,
QToolButton#toolButton_checkUpdate:disabled,QToolButton#toolButton_restart:disabled,QToolButton#toolButton_update:disabled,
QToolButton#pathBtn:disabled,QToolButton#resetDataBtn:disabled
{
    color:gray;
}
QToolButton#okBtn:pressed,QToolButton#cancelBtn:pressed,QToolButton#clearBtn:pressed,
QToolButton#newAccount:pressed,QToolButton#importKey:pressed,
QToolButton#toolButton_checkUpdate:pressed,QToolButton#toolButton_restart:pressed,QToolButton#toolButton_update:pressed,
QToolButton#pathBtn:pressed,QToolButton#resetDataBtn:pressed
{
    background-color:rgb(50,50,50);
}

QWidget#ContextWidget{
    background-color:black;
}

QDialog#WaitingForSync{
    border-image:url(:/pic/bg.png)
}

QTextEdit#waitSync{
    background-color: transparent;
    color: rgb(77,165,153);
    font: 14px \"Microsoft YaHei UI Light\";
    border:none;
    padding: 0px 10px 0px 6px;
}

QListView#completer{
    background-color:rgb(60,60,60);
    border:1px solid rgb(100,100,100);
    color:white;
    font:65 16px \"Microsoft YaHei UI\";
    outline:0px;
}

QWidget#MessageTypeShowWidget{
    background-color:rgb(45,45,45);
}
